Arithmetic {
  Exp = AddExp
      
  AddExp = AddExp_plus
         | AddExp_minus
         | MulExp

  MulExp = MulExp_times
         | MulExp_div
         | MulExp_power
         | number

  AddExp_plus = AddExp "+" MulExp

  AddExp_minus = AddExp "-" MulExp

  MulExp_times = MulExp "*" number

  MulExp_div = MulExp "/" number

  MulExp_power = MulExp "**" number

  number = decimal 
         | whole
          

  whole = digit+

  decimal = digit+ "." digit+
}
